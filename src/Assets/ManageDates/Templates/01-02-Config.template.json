{
  "Description": "Standard calendar with holidays",
  "Templates": [
    {
      "Class": "HolidaysDefinitionTable",
      "Table": "HolidaysDefinition",
      "Template": "HolidaysDefinition.json",
      "IsHidden": true
    },
    {
      "Class": "HolidaysTable",
      "Table": "Holidays",
      "Template": null
    },
    {
      "Class": "CustomDateTable",
      "Table": "Date",
      "ReferenceTable": "DateTemplate",
      "Template": "DateTemplate-01.json"
    }
  ],
  "FirstYear": 0,
  "LastYear": 9999,

  "AutoScan": "Full",
  "OnlyTablesColumns": [],
  "ExceptTablesColumns": [],

  "IsoCountry": "US",
  "InLieuOfPrefix": "(in lieu of ",
  "InLieuOfSuffix": ")",
  "WorkingDays": "{ 2, 3, 4, 5, 6 }",
  "HolidaysDefinitionTable": "HolidaysDefinition",
  "HolidaysReference": {
    "TableName": "Holidays",
    "DateColumnName": "Holiday Date",
    "HolidayColumnName": "Holiday Name"
  },

  "DefaultVariables": {
    "__FirstFiscalMonth": "4",
    "__FirstDayOfWeek": "0"
  },

  "Bravo": {
    "ReferencedTables": [
      {
        "Name": "Date",
        "Paths": [
          "$.Config.Templates[?(@.Class == 'CustomDateTable' && @.Table == 'Date')].Table"
        ]
      },
      {
        "Name": "DateTemplate",
        "Paths": [
          "$.Config.Templates[?(@.Class == 'CustomDateTable' && @.ReferenceTable == 'DateTemplate')].ReferenceTable"
        ]
      },
      {
        "Name": "Holidays",
        "Paths": [
          "$.Config.Templates[?(@.Class == 'HolidaysTable' && @.Table == 'Holidays')].Table",
          "$.Config.HolidaysReference.TableName"
        ]
      },
      {
        "Name": "HolidaysDefinition",
        "Paths": [
          "$.Config.Templates[?(@.Class == 'HolidaysDefinitionTable' && @.Table == 'HolidaysDefinition')].Table",
          "$.Config.HolidaysDefinitionTable"
        ]
      }
    ]
  }
}