<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <OutputType>Exe</OutputType> 
    <TargetFramework>net5.0-windows</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <ApplicationIcon>bravo.ico</ApplicationIcon>
    <StartupObject />
    <Company>SQLBI</Company>
    <Authors>SQLBI</Authors>
    <Product>Bravo for Power BI</Product>
    <Copyright>SQLBI Corp.</Copyright>
    <PackageIcon>icon-228.png</PackageIcon>
    <PackageIconUrl />
    <RootNamespace>Sqlbi.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\debug.js" />
    <Content Remove="wwwroot\index-debug.html" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dax.ViewModel" Version="1.0.36" />
    <PackageReference Include="Dax.Vpax.NetCore" Version="1.0.36" />
    <PackageReference Include="Photino.NET" Version="2.1.10" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

  <ItemGroup>
    <None Update="wwwroot\**">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="wwwroot\favicons\icon-228.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
	
</Project>
